<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="hibernateproj.Employee" table="employee">
<id name="emp_id" column="ID" type="integer">
	<generator class="assigned"></generator>
</id>
<property name="emp_name" column="NAME" type="string"></property>
<property name="emp_dept" column="DEPT" type="string"></property>
</class>
</hibernate-mapping>

