<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.excel.associations6.one2many2.StudentEntity" table="table_student12m">
		<id name="sid" type="java.lang.Long" column="sid">
			<generator class="assigned"></generator>
		</id>
		<property name="sname" type="java.lang.String" column="sname"></property>
	</class>

	<class name="com.excel.associations6.one2many2.CollegeEntity" table="table_college12m">
		<id name="collegecode" type="java.lang.Long" column="collegecode">
			<generator class="assigned" />
		</id>
		<property name="cname" type="java.lang.String" column="cname"></property>
		
		<set name="students">
			<key>
				<column name="collegecode_fk"></column>
			</key>
			<one-to-many class="com.excel.associations6.one2many2.StudentEntity" />
		</set>
	</class>
</hibernate-mapping>