<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.excel.associations6.one2manymany2one_3.StudentEntity" table="table_student12m">
		<id name="sid" type="java.lang.Long" column="sid">
			<generator class="assigned"></generator>
		</id>
		<property name="sname" type="java.lang.String" column="sname"></property>
		<many-to-one name="ce" class="com.excel.associations6.one2manymany2one_3.CollegeEntity"
			column="collegecode_fk"></many-to-one>
	</class>

	<class name="com.excel.associations6.one2manymany2one_3.CollegeEntity" table="table_college12mbi">
		<id name="collegecode" type="java.lang.Long" column="collegecode">
			<generator class="assigned" />
		</id>
		<property name="cname" type="java.lang.String" column="cname"></property>
		<set name="students" inverse="true" cascade="all" lazy="true" fetch="select">  <!-- fetch = select/join -->
			<key>
				<column name="collegecode_fk" ></column>
			</key>
			<one-to-many class="com.excel.associations6.one2manymany2one_3.StudentEntity" />
		</set>
	</class>






</hibernate-mapping>