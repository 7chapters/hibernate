<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.tutorial.hibernate.algorithms.sequence_oracle_2.StudentBean" table="student">
		<id name="sid">
			<generator class = "sequence">
				<param name = "sequence">STUDENT_SEQUENCE</param>
			</generator>
		</id>
		<property name="sname"/>
		<property name="tot_m"/>
	</class>
</hibernate-mapping>